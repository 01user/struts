<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE xwork PUBLIC 
	"-//OpenSymphony Group//XWork 1.1.1//EN"
	"http://www.opensymphony.com/xwork/xwork-1.1.1.dtd">
	
<xwork>
	<include file="struts-default.xml" />
	
	<package name="actionchaining" extends="struts-default" namespace="/actionchaining">
		<action name="actionChain1" class="org.apache.struts.action2.showcase.actionchaining.ActionChain1">
			<result type="chain">actionChain2</result>		
		</action>
		<action name="actionChain2" class="org.apache.struts.action2.showcase.actionchaining.ActionChain2">
			<result type="chain">actionChain3</result>
		</action>
		<action name="actionChain3" class="org.apache.struts.action2.showcase.actionchaining.ActionChain3">
			<result>/actionchaining/actionChainingResult.jsp</result>
		</action>
	</package>
</xwork>


